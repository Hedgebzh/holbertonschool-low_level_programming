#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/* more headers goes there */
/**
* main - Entry point
*
* Return: Always 0 (Success)
*/
int main(void)
{
char letter = 'a';
int x = '0';
while (x <='9')
{
putchar(x);
x=x+1;
}
while (letter <= 'f')
{
putchar(letter);
letter=letter+1;
}
return (0);
}
